{
    "name":"AppDigger",
    "version":"1.0.0",
    "description":"Collect some information of the app and submit it to the gathering page.",
    "permissions": [
        "tabs", "https://itunes.apple.com/*/app","http://itunes.apple.com/*/app", "https://itunes.apple.com/app","http://itunes.apple.com/app","https://play.google.com/store/apps/*","http://play.google.com/store/apps/"
    ],
    "browser_action": {
        "default_title": "Digger The App",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}